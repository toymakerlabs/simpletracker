package com.toymakerlabs.utils{	import flash.net.*;	import flash.events.Event;	import com.google.analytics.AnalyticsTracker;	import com.google.analytics.GATracker;	import flash.display.Stage;	import flash.display.DisplayObject;		public class SimpleTrackerGA	{		public const VIDEO_START	 :String = "video_start";		public const VIDEO_MIDPOINT  :String = "video_midpoint";		public const VIDEO_COMPLETE  :String = "video_complete";		public const AGEGATE_FAILURE :String = "age_fail";		public const EXTERNAL_LINK	 :String = "clickout";		public const EXPANSION	     :String = "expansion";		public const CONTRACTION	 :String = "contraction";				public var debug:Boolean = false;				private var tracker:AnalyticsTracker;		private var trackingCode:String;		private var placement:String;				public function SimpleTrackerGA(target:DisplayObject,GAID:String):void //"UA-18561091-1"		{			if(debug == true) {				trace("TRACKING:DEBUG");				tracker = new GATracker(target,GAID, "AS3", true);			}else {				tracker = new GATracker(target,GAID, "AS3", false);			}		}				public function set placementID(id:String):void		{			placement = id;		}				public function trackEvent (eventType:String):void 		{				if(debug == true) {				trace("Track : "+ eventType);			}else{				trace(eventType);				tracker.trackEvent(placement,eventType,"ad_event");			}		}	}}